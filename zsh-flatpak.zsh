
# Displaying installed flatpaks
alias flatpaks='flatpak list'
alias fp-ls='flatpaks'

# Update flatpaks
alias fp-up='flatpak update'

# Search flatpaks
alias fp-se='flatpak search'
alias fp-se-u='flatpak search --user'

# Install flatpaks
alias fp-in='flatpak install'
alias fp-in-u='flatpak isntall --user'

